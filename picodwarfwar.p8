	m={}
	m.x=0
	m.y=0
	c={}
	c.x =64
	c.y =64
	c.add = 8
	
	
	function _update()
  if (btnp(0)) then c.x=c.x-c.add end
	 if (btnp(1)) then c.x=c.x+c.add end
	 if (btnp(2)) then c.y=c.y-c.add end
	 if (btnp(3)) then c.y=c.y+c.add end
	end


	function draw_border()
		rect(82,2,126,82,2)
	end

	function _draw()
		rectfill(0,0,128,128,0)
	 map(0,0,m.x,m.y,10,10)
	 spr(32,c.x,c.y)
	 spr(18,4*8, 4*8)
	 draw_border()
	end

